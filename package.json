{
  "name": "todo-react-redux",
  "version": "0.6.0",
  "private": true,
  "description": "Todo app with Create React App, React Redux, and Firebase",
  "homepage": "https://todo-react-redux.firebaseapp.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/r-park/todo-react-redux.git"
  },
  "license": "MIT",
  "author": {
    "name": "Richard Park",
    "email": "objectiv@gmail.com"
  },
  "scripts": {
    "build": "run-s build:css build:js",
    "build:css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "build:js": "cross-env NODE_PATH=. node scripts/build.js",
    "start": "run-p start:css start:js",
    "start:css": "npm run build:css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start:js": "cross-env NODE_PATH=. node scripts/start.js",
    "test": "cross-env NODE_PATH=. node scripts/test.js --env=jsdom",
    "test:ci": "cross-env CI=true NODE_PATH=. node scripts/test.js --env=jsdom"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^1.1.0",
    "autoprefixer": "7.1.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "^3.0.1",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "firebase": "^4.1.3",
    "framework7": "^2.3.1",
    "fs-extra": "3.0.1",
    "history": "^4.6.3",
    "html-webpack-plugin": "2.29.0",
    "immutable": "^3.8.1",
    "jest": "20.0.4",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "promise": "7.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dev-utils": "^3.0.2",
    "react-dom": "^15.6.1",
    "react-error-overlay": "^1.0.9",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.3",
    "url-loader": "0.5.9",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.5.0",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "corber": "1.2.8",
    "corber-webpack-plugin": "^1.0.0",
    "cross-env": "^5.0.1",
    "enzyme": "^2.9.1",
    "firebase-tools": "^3.9.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "react-test-renderer": "^15.6.1",
    "sinon": "^2.3.8"
  },
  "engines": {
    "node": ">=8.1.4"
  }
}